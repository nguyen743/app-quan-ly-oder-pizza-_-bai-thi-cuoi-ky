<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Food">

    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/pizza"
        android:label="@string/app_name"
        android:roundIcon="@drawable/pizza"
        android:supportsRtl="true"
        android:theme="@style/BenBen_BenCS_CE150140">

        <activity
            android:name="com.example.Food.DangNhapAct"
            android:exported="false" />
        <activity
            android:name="com.example.Food.MHChaoAct"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.Food.MainActivity"
            android:exported="true" />
    </application>

</manifest>